{
  "name": "crls",
  "version": "2.0.0",
  "description": "Column and row-level security for TypeScript",
  "keywords": [
    "permissions",
    "access",
    "row level security",
    "rls",
    "column level security",
    "cls"
  ],
  "homepage": "https://crls.js.org",
  "bugs": "https://github.com/lukecarr/crls/issues",
  "repository": "lukecarr/crls",
  "author": "Luke Carr <me+oss@carr.sh>",
  "license": "MIT",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@parcel/packager-ts": "latest",
    "@parcel/transformer-typescript-types": "latest",
    "@vitest/coverage-c8": "latest",
    "dprint": "latest",
    "parcel": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitepress": "latest",
    "vitest": "latest",
    "vue": "latest"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@parcel/core"
      ]
    }
  },
  "packageManager": "pnpm@7.14.2"
}
